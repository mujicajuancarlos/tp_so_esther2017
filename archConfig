#ifndef KERNEL_MAIN_H_
#define KERNEL_MAIN_H_
#include <commons/config.h>

typedef struct{
	int PUERTO_PROG;
	int PUERTO_CPU;
	char IP_MEMORIA[15];
	int PUERTO_MEMORIA;
	char IP_FS[15];
	int PUERTO_FS;
	int QUANTUM;
	int QUANTUM_SLEEP;
	char ALGORITMO[4];
	int GRADO_MULTIPROG;
	// las sig variables son arrays
	char SEM_IDS[15];
	int SEM_INIT[15];
	char SHARED_VARS[15];
	int STACK_SIZE;


}archivoConfiguracion;

#endif /* KERNEL_MAIN_H_ */

